{% model "speakers.json" %}
{% block title %}Schedule{% endblock %}
{% block open-wrapper %}
{% endblock %}
{% block content %}
<div class="hero general__hero hero--short">
    <div class="wrapper">
        <h1>The day ahead</h1>
        <p class="subtitle subtitle__single"><span>A terrifying look into the world of tomorrow&hellip;</span></p>
    </div>
    <div class="general__hero-monster-dude"></div>
</div>
<main class="page-content">
    <div class="wrapper">
        <div class="schedule__grid-row">
            <div class="schedule__primary-grid-unit">
            <ol class="big-schedule-list">
            <li class="schedule-item">
                <div class="schedule-unit--time">
                    <div class="schedule__time">
                    8:30<span class="meridian">am</span>
                    </div>
                </div>
                <div class="schedule-unit--entry">
                    <h2 class="schedule__agenda-name">Registration</h2>
                    <div class="schedule__description">
                        <p>Collect your badge and conference bag, enjoy a chat and a coffee</p>
                    </div>
                </div>
            </li>
            <li class="schedule-item">
                <div class="schedule-unit--time">
                    <div class="schedule__time">
                    9:15<span class="meridian">am</span>
                    </div>
                </div>
                <div class="schedule-unit--entry">
                    <h2 class="schedule__agenda-name">Opening</h2>
                    <div class="schedule__description">
                        <p>Kirsty and Zach welcome you to the Shropgeek (R)Evolution</p>
                    </div>
                </div>
            </li>


            <li class="schedule-item">
                <div class="schedule-unit--time">
                    <div class="schedule__time">
                        9:30<span class="meridian">am</span>
                    </div>
                </div>
                {% for speaker in model.speaker limit:1 offset:2 %}
                    <div class="schedule-unit--entry">
                        <h2 class="schedule__speaker-name">
                            <a href="speakers#{{ speaker.bookmark_link }}">{{ speaker.firstname }} {{ speaker.surname }}</a>
                        </h2>
                        <p class="schedule__talk-title">{{ speaker.talk_title }}</p>
                        <div class="schedule__description">
                            {{ speaker.talk_description }}
                        </div>
                    </div>
                {% endfor %}
            </li>


            <li class="schedule-item">
